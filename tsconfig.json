{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@screens/*": [
        "src/screens/*"
      ],
      "@type/*": [
        "src/type/*"
      ],
      "@interfaces/*": [
        "src/interfaces/*"
      ],
      "@interceptors/*": [
        "src/interceptors/*"
      ],
      "@context/*": [
        "src/context/*"
      ],
      "@services/*": [
        "src/services/*"
      ]
    }
  },
  "typeRoots": [
    "./types"
  ]
}